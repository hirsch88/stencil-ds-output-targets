{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "declaration": true,
    "experimentalDecorators": true,
    "lib": ["dom", "es2017", "esnext.array"],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2017",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "../vue-next-output-target/dist",
    "jsx": "preserve"
  },
  "include": ["../vue-next-output-target/src"],
  "exclude": ["../vue-next-output-target/node_modules", "../vue-next-output-target/src/resources"]
}
