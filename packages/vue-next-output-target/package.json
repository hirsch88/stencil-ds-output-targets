{
  "name": "hirsch-vue-next-output-target",
  "version": "0.0.1",
  "description": "Vue 3 output target for @stencil/core components.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "vue-component-lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c",
    "version": "npm run build",
    "release": "np",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirsch88/stencil-ds-output-targets.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hirsch88/stencil-ds-output-targets/issues"
  },
  "peerDependencies": {
    "@stencil/core": ">=1.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts?|tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  },
  "gitHead": "a3588e905186a0e86e7f88418fd5b2f9531b55e0",
  "devDependencies": {
    "vue": "^3.0.5"
  }
}
